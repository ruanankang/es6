{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
        // "airbnb",
        // "plugin:vue/essential"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "Vue": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
        "vue","html"
    ],
    "rules": {
        "no-console": 0,
        "comma-dangle": 0
    }
}